{
  "expo": {
    "name": "food",
    "slug": "food",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "vhlongon.food",
      "versionCode": 1
    },
    "description": "study project for the react native course from Udemy",
    "githubUrl": "https://github.com/vhlongon/react-native-food"
  }
}
